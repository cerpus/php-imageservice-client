{
  "name": "cerpus/imageservice-client",
  "description": "Client to handle communication with the Cerpus ImageService",
  "require": {
    "illuminate/support": "^6.0|^7.0|^8.0",
    "nesbot/carbon": "^2.0",
    "cerpus/cerpushelper": "^1.4",
    "ext-json": "*"
  },
  "require-dev": {
    "fzaninotto/faker": "^1.7",
    "shrikeh/teapot": "^2.3",
    "phpunit/phpunit": "^8",
    "orchestra/testbench": "^4"
  },
  "authors": [
    {
      "name": "Thomas Horn Sivertsen",
      "email": "thomass@cerpus.com"
    },
    {
      "name": "Odd-Arne Johansen",
      "email": "odd-arne@gamilab.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Cerpus\\ImageServiceClient\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Cerpus\\ImageServiceClientTests\\": "tests"
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://composer.cerpus.net/"
    },
    {
      "type": "composer",
      "url": "https://composer-3rdparty.cerpus.net/"
    }
  ]
}
